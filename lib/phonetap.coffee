class @Camera
  DestinationType: {
    DATA_URL: 0
    FILE_URI: 1
  }
  EncodingType: {
    JPEG: 0
    PNG:  1
  }
  PictureSourceType: {
    PHOTOLIBRARY: 0
    CAMERA: 1
    SAVEDPHOTOALBUM: 2
  }

class @PhoneTap
  camera: {
    jpeg_data_url: ""

    getPicture: (success, fail, options) ->
      success(this.jpeg_data_url)

    getPictureFail: (success, fail, options) ->
      fail()
  }
